h1 POST
br
ul
  each post in posts
     li
      form(action='/user/post/liked', method='POST')
       span=post.content
       img(src=post.image)
       span=post.image
       span=post.likes
       button(name='postID', value=post.postID) LIKE  
h2 LIKEDPOST
br
ul
  each post in likedPosts
     li
      form(action='/user/post/unliked', method='POST')
       span=post.content
       img(src=post.image)
       span=post.image
       span=post.likes
       button(name='postID', value=post.postID) UNLIKE  
